    "keywords": ["graphs", "frenzy", "framework"],
    "homepage": "http://www.ironedgesoftware.com",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gustavo Falco",
            "email": "comfortablynumb84@gmail.com",
